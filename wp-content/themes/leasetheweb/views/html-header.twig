<!DOCTYPE html>
<!--[if lte IE 8 ]>
<html lang="en" class="no-js oldie">
<![endif]-->
<!--[if IE 9 ]>
<html lang="en" class="no-js ie9">
<![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <link rel="stylesheet" href="https://use.typekit.net/zha6oby.css">

  {% if options.head_block_pre_css %}
    {{ options.head_block_pre_css }}
  {% endif %}

  <style type="text/css">
    :root {
      --primary-color: {{ options.primary_color }};
      --primary-hover-color: {{ options.primary_button_hover_color }};
      --link-color: {{ options.link_color }};
      --link-hover-color: {{ options.link_hover_color }};
      --text-color-default: {{ options.default_text_color }};
      --text-color-inverted: {{ options.inverted_text_color }};
      --secondary-color: {{ options.secondary_color }};
      --black-color: {{ options.black }};
      --light-color: {{ options.light_color }};

      {% if options.primary_font %}
        --primary-font: {{ options.primary_font }}, sans-serif;
      {% else %}
        --primary-font: 'proxima-nova', sans-serif;
      {% endif %}
      {% if options.primary_font_regular_weight %}
        --primary-font-regular-weight: {{ options.primary_font_regular_weight }};
      {% else %}
        --primary-font-regular-weight: 400;
      {% endif %}
      {% if options.primary_font_medium_weight %}
        --primary-font-medium-weight: {{ options.primary_font_medium_weight }};
      {% else %}
        --primary-font-medium-weight: 500;
      {% endif %}
      {% if options.primary_font_bold_weight %}
        --primary-font-bold-weight: {{ options.primary_font_bold_weight }};
      {% else %}
        --primary-font-bold-weight: 700;
      {% endif %}

      {% if options.button_style == 'square' %}
        --button-border-radius: 0;
      {% else %}
        --button-border-radius: 28px;
      {% endif %}

      {% if options.button_a_color %}
        --button-a-color: {{ options.button_a_color }};
      {% endif %}
      {% if options.button_a_hover_color %}
        --button-a-hover-color: {{ options.button_a_hover_color }};
      {% endif %}
      {% if options.button_b_color %}
        --button-b-color: {{ options.button_b_color }};
      {% endif %}
      {% if options.button_b_hover_color %}
        --button-b-hover-color: {{ options.button_b_hover_color }};
      {% endif %}
      {% if options.button_c_color %}
        --button-c-color: {{ options.button_c_color }};
      {% endif %}
      {% if options.button_c_hover_color %}
        --button-c-hover-color: {{ options.button_c_hover_color }};
      {% endif %}
    }
  </style>

  <link rel="stylesheet" href="{{theme.path}}/style.css" type="text/css" media="screen">

  {% if options.head_block_post_css %}
    {{ options.head_block_post_css }}
  {% endif %}

  {{function('wp_head')}}
